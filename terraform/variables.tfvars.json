{
  "AWS_REGION": "ap-northeast-3",
  "AWS_PROJECT": "VPN",
  "AWS_ENVIRONMENT": "Development",
  "AWS_GROUP": "Network",
  "AWS_DYNAMODB_TABLES": [
    {
      "NAME": "terraform-state",
      "GROUP": "Storage",
      "HASH_KEY": "LockID",
      "ATTRIBUTE": {
        "NAME": "LockID",
        "TYPE": "S"
      },
      "READ_CAPACITY": "20",
      "WRITE_CAPACITY": "20"
    }
  ],
  "AWS_KMS_KEYS": [
    {
      "NAME": "kms_key",
      "GROUP": "Storage",
      "DESCRIPTION": "Chave KMS para encriptar terraform state",
      "DELETION_WINDOW_IN_DAYS": "10",
      "ENABLE_KEY_ROTATION": "true"
    }
  ],
  "AWS_KMS_KEY_ALIASES": [
    {
      "NAME": "alias/key",
      "TARGET_KEY_ID": "kms_key"
    }
  ],
  "AWS_S3_BUCKETS": [
    {
      "NAME": "terraform-state-20231103",
      "GROUP": "Storage",
      "BLOCK": {
        "BLOCK_PUBLIC_ACLS"       : "true",
        "BLOCK_PUBLIC_POLICYS"    : "true",
        "IGNORE_PUBLIC_ACLS"      : "true",
        "RESTRICT_PUBLIC_BUCKETS" : "true"
      },
      "VERSIONING": {
        "STATUS": "Enabled"
      },
      "ENCRYPTION": {
        "SSE_ALGORITHM": "aws:kms",
        "KEY": "alias/key"
      }
    }
  ]
